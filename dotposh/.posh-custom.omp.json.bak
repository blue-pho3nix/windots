{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "Rosewater": "#f5e0dc",
        "Flamingo": "#f2cdcd",
        "Pink": "#f5c2e7",
        "Mauve": "#cba6f7",
        "Red": "#f38ba8",
        "Maroon": "#eba0ac",
        "Peach": "#fab387",
        "Yellow": "#f9e2af",
        "Green": "#a6e3a1",
        "Teal": "#94e2d5",
        "Sky": "#89dceb",
        "Sapphire": "#74c7ec",
        "Blue": "#89b4fa",
        "Lavender": "#b4befe",
        "Text": "#cdd6f4",
        "Subtext1": "#bac2de",
        "Subtext0": "#a6adc8",
        "Overlay2": "#9399b2",
        "Overlay1": "#7f849c",
        "Overlay0": "#6c7086",
        "Surface2": "#585b70",
        "Surface1": "#45475a",
        "Surface0": "#313244",
        "Base": "#1e1e2e",
        "Mantle": "#181825",
        "Crust": "#11111b"
    },
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:Lavender",
                    "properties": {
                        "alpine": "\uf300",
                        "arch": "\uf303",
                        "centos": "\uf304",
                        "debian": "\uf306",
                        "elementary": "\uf309",
                        "fedora": "\uf30a",
                        "gentoo": "\uf30d",
                        "linux": "\ue712",
                        "macos": "\ue711",
                        "manjaro": "\uf312",
                        "mint": "\uf30f",
                        "opensuse": "\uf314",
                        "raspbian": "\uf315",
                        "ubuntu": "\uf31c",
                        "windows": "\ue70f"
                    },
                    "style": "plain",
                    "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}  ",
                    "type": "os"
                },
                {
                    "foreground": "p:Pink",
                    "properties": {
                        "folder_icon": "..\ue5fe..",
                        "home_icon": "~",
                        "style": "agnoster_short"
                    },
                    "style": "plain",
                    "template": "{{ .Path }} ",
                    "type": "path"
                },
                {
                    "foreground": "p:Peach",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}p:Yellow{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:Maroon{{ end }}",
                        "{{ if gt .Ahead 0 }}p:Green{{ end }}",
                        "{{ if gt .Behind 0 }}p:Red{{ end }}"
                    ],
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_worktree_count": true,
                        "branch_icon": "\ue725 ",
                        "cherry_pick_icon": "\ue29b ",
                        "commit_icon": "\uf417 ",
                        "merge_icon": "\ue727 ",
                        "no_commits_icon": "\uf0c3 ",
                        "rebase_icon": "\ue728 ",
                        "revert_icon": "\uf0e2 ",
                        "tag_icon": "\uf412 ",
                        "mapped_branches": {
                            "feat/*": "üöÄ ",
                            "bug/*": "üêõ "
                        }
                    },
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Û∞Üì {{ .StashCount }}{{ end }} ",
                    "type": "git"
                },
                // {
                //     "type": "gitversion",
                //     "style": "diamond",
                //     "leading_diamond": "\u250b",
                //     "foreground": "p:Teal",
                //     "template": " Ôáí {{ .MajorMinorPatch }} ",
                //     "cache_timeout": 0
                // },
                // {
                //     "type": "docker",
                //     "style": "diamond",
                //     "leading_diamond": "\u250b",
                //     "foreground": "p:Blue",
                //     "template": " Ôåà {{ .Context }} "
                // },
                // {
                //     "type": "aws",
                //     "style": "diamond",
                //     "leading_diamond": "\u250b",
                //     "foreground": "p:Peach",
                //     "template": " Óû≠ {{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
                // },
                // {
                //     "type": "kubectl",
                //     "style": "diamond",
                //     "leading_diamond": "\u250b",
                //     "foreground": "p:Teal",
                //     "template": " ‚ò∏{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
                // },
                // {
                //     "type": "terraform",
                //     "style": "diamond",
                //     "leading_diamond": "\u250b",
                //     "foreground": "p:Yellow",
                //     "template": "{{.WorkspaceName}}"
                // },
                {
                    "type": "npm",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": "Óúû {{ .Full }} ",
                    "foreground": "p:Yellow"
                },
                {
                    "type": "bun",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "foreground": "p:Blue",
                    "template": " ü•ü{{ .Full }} "
                },
                {
                    "type": "pnpm",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "foreground": "p:Peach",
                    "template": " Û∞ãÅ {{ .Full }} "
                },
                {
                    "type": "yarn",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "foreground": "p:Sapphire",
                    "template": " Û∞Ñõ {{ .Full }} "
                },
                {
                    "type": "react",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "foreground": "p:Sky",
                    "template": " Óû∫ {{ .Full }} "
                },
                {
                    "type": "dotnet",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": " Óùø {{ .Full }} ",
                    "foreground": "p:Red"
                },
                {
                    "type": "lua",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": " Óò† {{ .Full }} ",
                    "foreground": "p:Sky"
                },
                {
                    "type": "node",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": " Óúò {{ .Full }} ",
                    "foreground": "p:Green"
                },
                {
                    "type": "php",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "foreground": "p:Blue",
                    "template": " ÓúΩ {{ .Full }} "
                },
                {
                    "type": "python",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": " Óàµ {{ .Full }} ",
                    "foreground": "p:Yellow"
                },
                {
                    "type": "ruby",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": " Óûë {{ .Full }} ",
                    "foreground": "p:Red"
                },
                {
                    "type": "rust",
                    "style": "diamond",
                    "leading_diamond": "\u250b",
                    "template": " Óû® {{ .Full }} ",
                    "foreground": "p:Red"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                // {
                //     "type": "project",
                //     "style": "diamond",
                //     "trailing_diamond": "\u250b ",
                //     "foreground": "p:Yellow",
                //     "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}Ôíá {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
                // },
                {
                    "foreground": "p:Green",
                    "style": "diamond",
                    "template": "RAM:{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB ",
                    "trailing_diamond": " ",
                    "type": "sysinfo"
                },
                {
                    "foreground": "p:Blue",
                    "leading_diamond": "\u250b",
                    "properties": {
                        "style": "roundrock",
                        "threshold": 0,
                        "always_enabled": true
                    },
                    "style": "diamond",
                    "template": " {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "foreground": "p:Pink",
                    "properties": {
                        "time_format": "15:04"
                    },
                    "leading_diamond": "\u250b",
                    "style": "diamond",
                    "template": " {{ .CurrentDate | date .Format }}",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "spotify",
                    "style": "diamond",
                    "leading_diamond": "\ue0c7\ue0c7\ue0c7",
                    "trailing_diamond": "ÓÉÄ ",
                    "foreground": "p:Sapphire",
                    "background": "p:Surface1",
                    "template": "  \uf1bc {{ if ne .Status \"stopped\" }} {{ .Artist }} ‚ú¶  {{ .Track }}{{ end }} "
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "style": "plain",
                    "template": "\u276f",
                    "type": "text",
                    "foreground": "p:Sky",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}p:Red{{ end }}"
                    ]
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}